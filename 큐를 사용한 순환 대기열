//큐를 사용한 순환 대기열
#include <iostream>
#include <queue>
#include <string>
int main() {

	std::string command;
	//char command[10];

	std::queue<std::string> Queue;

	//printf("command : enqueue , dequeue , print , quit : ");
	//std::cin >> command;
	//std::cout << command << std::endl;
	std::string word;

	while (true) {
		printf("command : enqueue , dequeue , print , quit : ");
		std::cin >> command;
		if (command == "enqueue") {
			std::cout << "word = " ;
			std::cin >> word;
			if (Queue.size() != 4) { // 큐에 4개가 아니라면 
				Queue.push(word);
				continue;
			}
			else {		//큐가 4개로 꽉 찼다면.
				printf("Queue Full\n");
				continue;
			}
		}

		else if (command == "dequeue") {
			if (Queue.size() != 0) {	//큐에 요소가 남아있다면.
				Queue.pop();
				continue;
			}
			else {
				printf("Queue Empty\n");
				continue;
			}
		}

		else if (command == "print") {
			int number = Queue.size();
			for (int i = 0; i < number; ++i) {
				std::cout << Queue.front() << std::endl;
				Queue.pop();
			}
			continue;
		}
		else if (command == "quit") {
			printf("종료\n");
			break;
		}
		else  {
			printf("Bad Command\n");
			break;
		}


	}

}
